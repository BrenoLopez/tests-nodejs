{
  "compilerOptions": {
    "target": "ESNext",
    "module": "commonjs",
    "experimentalDecorators": true,
    "rootDir": "./",
    "baseUrl": "./", 
    "outDir": "./dist",
    "sourceMap": true,
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "strict": true, 
    "skipLibCheck": true,
    "paths": {
      "@/*": ["./src/*"],
      "@core/*":["./src/core/*"]
    },                          
  }
}
